{
  "name": "shoukaku",
  "version": "4.0.0-dev",
  "description": "A stable and updated wrapper around Lavalink",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && npm run build:esm && npm run build:docs",
    "build:esm": "gen-esm-wrapper dist/index.js dist/index.mjs",
    "build:docs": "typedoc --theme default --readme README.md --out docs/ --entryPointStrategy expand src/.",
    "lint": "eslint --fix --ext .ts",
    "prepare": "tsc && npm run build:esm"
  },
  "keywords": [
    "bot",
    "music",
    "lavalink",
    "api",
    "discord",
    "lavalink.js",
    "discord.js",
    "lavalink-api",
    "weeb-library"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  },
  "author": "Saya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deivu/Shoukaku"
  },
  "dependencies": {
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@augu/eslint-config": "^4.0.1",
    "@types/node": "^18.13.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "gen-esm-wrapper": "^1.1.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.25",
    "typescript": "^4.9.5"
  }
}
